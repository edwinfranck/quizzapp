{
  "expo": {
    "name": "quizz-app",
    "slug": "quizz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/res/mipmap-xxxhdpi/ic_launcher_monochrome.png",
    "scheme": "quizzapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/logos/res/mipmap-xxxhdpi/ic_launcher_monochrome.png",
        "backgroundImage": "./assets/logos/res/mipmap-xxxhdpi/ic_launcher_monochrome.png",
        "monochromeImage": "./assets/logos/res/mipmap-xxxhdpi/ic_launcher_monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.edwind4.quizzapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logos/res/mipmap-xxxhdpi/ic_launcher_monochrome.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logos/Grade.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "063365a1-1400-48b1-be83-402dcbe21e94"
      }
    }
  }
}